        <a href="/" class="navbar-brand">Productkart</a>
        <a href="/">
          <img class="logo" src="https://cdn0.iconfinder.com/data/icons/ecommerce-38/34/7-64.png" alt="">
        </a>
      </a>
      <form class="d-flex" role="search">
        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
        <div id='button-holder'>
          <a href="#">
            <img class="searchicon"
              src='https://cdn4.iconfinder.com/data/icons/symbol-blue-set-1/100/Untitled-2-12-64.png' />
          </a>
        </div>
        <button class="btn" type="submit" ><a href="/login">login</a></button>
        <button class="btn2" type="submit"><a href="/register">Register</a></button>
      </form>
    </div>
